




pipeline{
  agent any
      stages{
        stage('Compile'){
            withMaven(maven : 'Maven')
            sh 'mvn clean compile'
            }
            
        stage('Test'){
            withMaven(maven : 'Maven')
            sh 'mvn test'
            }
            
            stage('Deploy'){
            withMaven(maven : 'Maven')
            sh 'mvn deploy'
            }
            }
           }
         
      
